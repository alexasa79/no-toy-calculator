{
  "name": "toy-calculator",
  "displayName": "Toy Calculator",
  "description": "",
  "repository": "git@github.com:alexasa79/toy-calculator.git",
  "version": "0.0.8",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "toy-calculator.eval",
        "title": "Toy Calculator: Evaluate"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
